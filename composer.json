{
    "name": "fortytwo-studio/craft-cms-boilerplate",
    "description": "This is a boilerplate for Craft CMS",
    "license": "MIT",
    "require": {
        "adigital/cookie-consent-banner": "1.2.9",
        "craftcms/aws-s3": "1.3.0",
        "craftcms/cms": "3.7.63",
        "craftcms/redactor": "2.10.11",
        "ether/seo": "3.7.4",
        "solspace/craft-freeform": "3.13.22.1",
        "spicyweb/craft-neo": "2.13.15",
        "verbb/buttonbox": "^3.1",
        "verbb/navigation": "1.4.31",
        "verbb/super-table": "2.7.4",
        "vlucas/phpdotenv": "^3.4.0",
        "wrav/oembed": "1.13.19"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.5"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "*": true,
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            "./craft setup/security-key",
            "./craft setup/app-id",
            "./craft setup/db",
            "./craft db/restore ./storage/setup-boilerplate/default_db.sql",
            "npm install --silent",
            "php ./storage/setup-boilerplate/after_setup.php"
        ]
    }
}
