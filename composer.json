{
  "name": "fortytwo-studio/craft-cms-boilerplate",
  "description": "This is a boilerplate for Craft CMS",
  "license": "MIT",
  "require": {
    "craftcms/aws-s3": "^1.2.11",
    "craftcms/cms": "^3.6.4",
    "craftcms/redactor": "^2.8.5",
    "ether/seo": "^3.6.7",
    "solspace/craft-freeform": "^3.10.11",
    "spicyweb/craft-neo": "^2.9.5",
    "supercool/buttonbox": "^2.0.4",
    "verbb/navigation": "^1.4.15",
    "verbb/super-table": "^2.6.7",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "./craft setup/app-id",
      "./craft setup/security-key",
      "./craft setup/db",
      "./craft db/restore ./storage/setup-boilerplate/default_db.sql",
      "npm install",
      "php ./storage/setup-boilerplate/after_setup.php"
    ]
  }
}
