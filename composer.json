{
    "name": "fortytwo-studio/craft-cms-boilerplate",
    "description": "This is a boilerplate for Craft CMS",
    "license": "MIT",
    "require": {
        "craftcms/aws-s3": "2.2.3",
        "craftcms/ckeditor": "4.11.0",
        "craftcms/cms": "5.8.21",
        "ether/seo": "5.0.0",
        "fortytwo-studio/cookie-consent": "^3.1",
        "johnhenry/matrix-block-anchor": "3.0.0",
        "nystudio107/craft-vite": "^5.0.1",
        "solspace/craft-freeform": "5.14.1",
        "topshelfcraft/environment-label": "^5.0.0",
        "verbb/navigation": "3.0.15",
        "vlucas/phpdotenv": "^3.4.0",
        "wrav/oembed": "3.1.6"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.5"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "*": true,
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            "./craft setup/security-key",
            "./craft setup/app-id",
            "./craft setup/db",
            "./craft db/restore ./storage/setup-boilerplate/default_db.sql",
            "npm install --silent",
            "php ./storage/setup-boilerplate/after_setup.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}
