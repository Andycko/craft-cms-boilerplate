{% extends "_layout" %}

{% block content %}

    {% if entry.pageHeader.count() %}
        {% set header = entry.pageHeader.one %}
        {% include '_includes/_page-headers/_' ~ header.type.handle %}
    {% endif %}

    {% if entry.contentBuilder %}
        {% for block in entry.contentBuilder.all() %}
            {% include '_includes/_content-builder/_blocks/' ~ block.type.handle %}
        {% endfor %}
    {% endif %}

{% endblock %}