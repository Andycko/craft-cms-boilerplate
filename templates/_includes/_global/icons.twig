{#
	Partial: Favicons
	========================================
	Favicons to be inherited by all layouts and templates across the project.
#}

{% if craft.vite.devServerRunning() %}
	<link rel="shortcut icon" href="{{ craft.vite.asset('/src/images/favicon.svg') }}" />
{% else %}
	{{ craft.vite.inline('@webroot/dist/favicons/favicons.html') }}
{% endif %}